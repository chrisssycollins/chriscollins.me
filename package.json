    "autoprefixer": "^9.4.7",
    "axios": "^0.18.0",
    "clean-css": "^4.2.1",
    "concurrently": "^4.1.0",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-connect": "^5.7.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-uglify": "^3.0.1",
    "gulp-watch": "^5.0.1",
    "html-minifier": "^3.5.21",
