{
  "name": "chrisssy",
  "description": "Personal blog/site",
  "version": "1.0.0",
  "author": "Chris Collins <chriscollins238@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.247",
    "gatsby-link": "^1.6.40",
    "gatsby-plugin-react-helmet": "^2.0.10",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-styled-components": "^2.0.11",
    "react-feather": "^1.1.0",
    "react-helmet": "^5.2.0",
    "react-medium-image-zoom": "^3.0.11",
    "styled-components": "^3.3.0"
  },
  "keywords": [
    "gatsby",
    "react"
  ],
  "license": "MIT",
  "scripts": {
    "build": "tailwind build ./src/styles/application.css -c ./tailwind.config.js -o ./src/static/application.css && purgecss -c purgecss.config.js -o ./src/static && gatsby build",
    "dev": "tailwind build ./src/styles/application.css -c ./tailwind.config.js -o ./src/static/application.css && gatsby develop",
    "format": "prettier --write 'src/**/*.js'",
    "deploy": "tailwind build ./src/styles/application.css -c ./tailwind.config.js -o ./src/static/application.css && purgecss -c purgecss.config.js -o ./src/static && gatsby build && cp ./CNAME public/ && gh-pages -d public",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "medium-zoom": "^0.4.0",
    "prettier": "^1.12.0",
    "purgecss": "^1.0.0",
    "tailwindcss": "^0.5.3"
  }
}
