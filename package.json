{
  "name": "jamstack-web-starter",
  "version": "1.5.0",
  "description": "Minimal boilerplate for new projects built with Eleventy, Tailwind, PurgeCSS and PostCSS",
  "scripts": {
    "media": "copyfiles src/_hiking/**/*.jpeg out",
    "start": "concurrently \"postcss src/_assets/stylesheets/app.css -o src/static/app.compiled.css --watch --verbose\" \"webpack --watch --config webpack.config.js\" \"eleventy --serve --watch\"",
    "build": "del dist && eleventy && NODE_ENV=production postcss src/_assets/stylesheets/app.css -o src/_includes/app.compiled.css && webpack -p --config webpack.config.js && ELEVENTY_ENV=production eleventy"
  },
  "author": "Chris Collins",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@11ty/eleventy-plugin-rss": "^1.0.7",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "colorthief": "^2.3.0",
    "concurrently": "^5.2.0",
    "copyfiles": "^2.2.0",
    "del-cli": "^3.0.0",
    "dotenv": "^8.2.0",
    "eleventy-plugin-reading-time": "0.0.1",
    "fast-average-color": "^6.0.2",
    "fast-glob": "^3.2.2",
    "html-minifier": "^4.0.0",
    "image-size": "^0.8.3",
    "js-yaml": "^3.13.1",
    "luxon": "^1.24.1",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.7",
    "markdown-it-toc-done-right": "^4.1.0",
    "node-fetch": "^2.6.1",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-nested": "^4.2.1",
    "purgecss": "^2.2.1",
    "sanitize-html": "^1.23.0",
    "tailwindcss": "^1.4.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
